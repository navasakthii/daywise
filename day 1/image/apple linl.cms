#stl_loader {
  position: absolute;
  background: transparent;
  z-index: 999;
  width: calc(100% - 20px);
  height: 100%;
}
#stl_loader.hideLoader{
    display:none;
}
#stl_loader::before {
    content: '';
    border-radius: 50%;
    width: 10px;
    height: 10px;
    border: 0.25rem solid rgba(0, 0, 0, 0.8);
    border-top-color: rgba(255,255,255,0.7);
    -webkit-animation: affspin 1s infinite linear;
    animation: affspin 1s infinite linear;
    position: absolute;
    top: calc(50% - 13px);
    left: calc(50% - 24px);
    z-index: 9;
  }
 #stl_loader::after {
    content: '';
    position: absolute;
    top: 0;
    bottom: 0;
    right: 0;
    left: 0;
  }
 .blink-button.showLoader {    
    background: #9b9b9b;
    color: rgb(155 ,155 ,155,0.8);
 }
 .blink-button.showLoader::before, .blink-button.showLoader::after { display:none; }

.blink-button{
  background: #fff;
  color: #1a1a1a;
  text-transform: capitalize;
  font-size: 12px;
  line-height: 16px;
  font-weight: 600;
  text-decoration: none;
  padding: 4px 7px 4px 24px;
  position: absolute;
  right: 12px;
  top: 12px;
  z-index: 1;
  border-radius: 4px;
  cursor: pointer;
}
.blink-button::before, .blink-button::after{
    opacity: 0;
    box-sizing: border-box;
    content: '\0020';
    position: absolute;
    top: 7px;
    left: 8px;
    width: 10px;
    height: 10px;
    border: 2px solid #e21b22;
    box-shadow: 0 0 10px #fff, inset 0 0 10px #e21b22;
    border-radius: 100px;
  }
 .blink-button::before {
    z-index: 2;
    -webkit-animation: blinker 2s infinite;
    -moz-animation: blinker 2s infinite;
    animation: blinker 2s infinite;
  } 
 .blink-button::after {
    z-index: 1;
    -webkit-animation: blinker 2s infinite 1s;
    -moz-animation: blinker 2s infinite 1s;
    animation: blinker 2s infinite 1s;
  } 
  
.blink-button.hide{ display: none;}
/* for animation of blink button */
@keyframes blinker {
  1% {
    -webkit-transform: scale(0);
    opacity: 0;
  }

  50% {
    opacity: 1;
  }

  100% {
    -webkit-transform: scale(1);
    opacity: 0;
  }
}

@-webkit-keyframes blinker {
  1% {
    -webkit-transform: scale(0);
    opacity: 0;
  }

  50% {
    opacity: 1;
  }

  100% {
    -webkit-transform: scale(1);
    opacity: 0;
  }
}

@-moz-keyframes blinker {
  1% {
    -webkit-transform: scale(0);
    opacity: 0;
  }

  50% {
    opacity: 1;
  }

  100% {
    -webkit-transform: scale(1);
    opacity: 0;
  }
}

